  // Dictionnaire personnalisé pour éviter les erreurs de frappe
  "cSpell.userWords": [
    "appli",
    "applis",
    "appliquer",
    "appliquées",
    "approuvé",
    "approche",
    "apprendre",
    "représente",
    "représenté",
    "représentée",
    "représentées",
    "représentés",
    "rétractable"
  ],

  // Formatage automatique
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },

  // Prettier comme formateur principal
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  // ESLint
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.alwaysShowStatus": true,

  // Tailwind CSS IntelliSense
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.experimental.classRegex": [
    "className=\"([^\"]*)",
    "className={`([^`]*)`",
    "tw`([^`]*)`"
  ],

  // Désactive les warnings sur @tailwind dans CSS
  "css.lint.unknownAtRules": "ignore",

  // GitLens
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": true,

  // Affichage
  "workbench.colorTheme": "JellyFish",
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "onFocusChange",

  // Divers
  "explorer.confirmDelete": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.inlineSuggest.enabled": true,

  // Optionnel : supprime le formatage auto pour Markdown
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "postman.settings.dotenv-detection-notification-visibility": false,
  "compile-hero.disable-compile-files-on-did-save-code": false
}
